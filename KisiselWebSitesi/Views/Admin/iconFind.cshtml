@model KisiselWebSitesi.Models.Classes.Icon

@{
    ViewBag.Title = "iconFind";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>İkon Güncelee</h2>
@using (Html.BeginForm("iconUpdate", "Admin", FormMethod.Post))
{
    @Html.LabelFor(x => x.id)
    @Html.TextBoxFor(x => x.id, new { @class = "form-control", @readonly = "readonly" })
    <br />
    @Html.LabelFor(x => x.ikon)
    @Html.TextBoxFor(x => x.ikon, new { @class = "form-control", @readonly = "readonly" })
    <br />
    @Html.LabelFor(x => x.link)
    @Html.TextBoxFor(x => x.link, new { @class = "form-control", @readonly = "readonly" })
	<br />
	<button class="btn btn-success">İkon Güncelle</button>

}

